{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "title": "Implementation comparison (n=2048)",
  "config": {
    "font": "Spectral, sans-serif"
  },
  "autosize": "fit",
  "width": 500,
  "height": 300,
  "data": {
    "url": "./implementation-comparison-fixed-n.csv"
  },
  "encoding": {
    "x": {
      "field": "threads",
      "type": "quantitative",
      "sort": "ascending",
      "scale": {
        "domain": [
          0,
          8
        ]
      },
      "title": "Number of threads"
    },
    "y": {
      "field": "duration",
      "aggregate": "mean",
      "type": "quantitative",
      "title": "Mean duration (seconds)"
    },
    "color": {
      "field": "name",
      "title": "Implementation",
      "type": "nominal"
    }
  },
  "layer": [
    {
      "mark": { "type": "point", "opacity": 0.5 }
    },
    {
      "mark": { "type": "line", "tooltip": true, "point": "transparent" },
      "encoding": {
        "y": {
          "field": "duration",
          "aggregate": "mean"
        }
      }
    }
  ]
}
